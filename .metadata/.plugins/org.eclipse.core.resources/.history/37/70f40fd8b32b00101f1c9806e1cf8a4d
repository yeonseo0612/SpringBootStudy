package com.korea.product.model;

import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Data;
import lombok.NoArgsConstructor;

@Builder
@AllArgsConstructor
@NoArgsConstructor
@Data
public class ProductDTO {

	private Long id;
	
	private String name;
	
	private String description;
	
	private int price;
	
}
